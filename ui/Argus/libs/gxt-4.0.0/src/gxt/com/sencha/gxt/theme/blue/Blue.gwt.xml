  </replace-with>

  <!-- Paging tool bar -->
  <replace-with class="com.sencha.gxt.theme.blue.client.toolbar.BluePagingToolBarAppearance">
    <when-type-is class="com.sencha.gxt.widget.core.client.toolbar.PagingToolBar.PagingToolBarAppearance" />
