  <!-- Status proxy -->
  <replace-with class="com.sencha.gxt.theme.blue.client.statusproxy.BlueStatusProxyAppearance">
    <when-type-is class="com.sencha.gxt.dnd.core.client.StatusProxy.StatusProxyAppearance" />
